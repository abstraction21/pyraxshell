auth
    is_authenticated
    exit
databases
    # List Cloud Databases instances, flavours
    list
    list_flavors
    # Create a new instance
    create_instance name:foodbs volume:1 flavor_id:1
    # Create a new Cloud Database
    create_database database_name:bar instance_name:foodbs
    exit
servers
